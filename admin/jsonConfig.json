{
	"i18n": true,
	"type": "panel",
	"items": {
		"host": {
			"type": "text",
			"label": "Printer IP address",
			"newLine": true
		},
		"Password": {
			"type": "password",
			"label": "Access Token (see printer network settings)",
			"newLine": true,
			"sm": 3
		},
		"serial": {
			"type": "text",
			"label": "Serial number of device (see printer network settings)",
			"newLine": false,
			"sm": 3
		},
		"printerModel": {
			"newLine": true,
			"type": "select",
			"label": "Printer Model",
			"sm": 3,
			"md": 3,
			"lg": 3,
			"options": [
				{
					"label": "P1P",
					"value": "P1P"
				},
				{
					"label": "P1s",
					"value": "P1s"
				},
				{
					"label": "X1",
					"value": "X1"
				}
			],
			"default": "X1",
			"help": "Please select your printer model"
		},
		"requestInterval": {
			"type": "number",
			"label": "Seconds between data polling",
			"help": "Only used for P1x series",
			"sm": 1,
			"default": 5,
			"min" : 1,
			"max": 36000
		}
	}
}
